int solution(vector<int> scoville, int K) {
    int answer = 0;
    priority_queue <int, vector<int>, greater<int>> que(scovile.begin(), scovile.end());//우선순위 큐 만드는 방법 정확히 인지 
    while(que.size() > 1 && que.top() < k )
    {
        int first = que.top();
        que.pop();
        int second = que.top();
        que.pop();
        que.push(first);
        answer__;
    }
    if(que.top()<K)
        return -1;
    return answer;
}
